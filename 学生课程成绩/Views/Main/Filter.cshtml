@{
}


@ViewBag.stu.RelName

<table>
    <thead>
        <tr>
            <th>课程名</th>
            <th>分数</th>
            <th>是0否1选修</th>
            <th>寒假0暑假1</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var gp in ViewBag.Gp)
        {
            <tr>
                <td>@gp.Class.Name</td>
                <td>@gp.Point</td>
                <td>@gp.Type</td>
                <td>@gp.Season</td>
            </tr>
        }
    </tbody>
</table>
